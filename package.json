{
  "name": "gym-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint",
    "check": "biome check",
    "format": "biome format",
    "fix": "biome check --write && biome format --write && biome lint --write",
    "prepare": "husky",
    "types:check": "tsc --noEmit",
    "db:migrate:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "tsx ./src/db/migrate.ts",
    "db:migrate:prod": "dotenv -e .env.prod -- drizzle-kit migrate --config=drizzle.config.ts",
    "db:migrate:rollback": "drizzle-kit down --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio",
    "db:clear": "tsx ./src/db/clear.ts",
    "db:seed": "tsx ./src/db/seed.ts",
    "db:reset": "npm run db:clear && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "0.13.8",
    "@tanstack/react-query": "^5.84.2",
    "@tanstack/react-query-devtools": "^5.84.2",
    "arctic": "3.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "17.2.0",
    "drizzle-orm": "0.44.3",
    "jose": "6.0.12",
    "lucide-react": "^0.539.0",
    "next": "15.4.1",
    "postgres": "3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-icons": "5.5.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "4.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@tailwindcss/postcss": "4",
    "@types/node": "20",
    "@types/react": "19",
    "@types/react-dom": "19",
    "drizzle-kit": "0.31.4",
    "husky": "9.1.7",
    "lint-staged": "16.1.2",
    "tailwindcss": "4",
    "tsx": "4.20.3",
    "typescript": "5"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "biome check --write",
      "biome format --write",
      "biome lint --write"
    ]
  }
}
